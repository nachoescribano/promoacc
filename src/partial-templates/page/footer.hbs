<footer class="footer">
  <!-- footer -->
  <div class="container footer__symbol">
    <div class="row">
      <div class="col-12 col-md-6 footer__container">
        <div class="footer__container-stamp">
          <img
            class="footer__stamp"
            src="assets/images/sello-LRQA@2x.png"
            alt="{{footer.image-alt-1}}"
            width="157px"
            height="170px"
          />
        </div>
        {{#each footer.links}}
          <ul class="footer__list">
            {{#each this}}
              <li class="footer__list-item">
                <a href="{{this.href}}" class="footer__list-item-link un">
                  {{#if this.rss}}
                    <svg
                      class="footer__list-item-icon"
                      xmlns="http://www.w3.org/2000/svg"
                      width="14"
                      height="14"
                      fill="currentColor"
                      class="bi bi-rss"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z"
                      ></path>
                      <path
                        d="M5.5 12a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm-3-8.5a1 1 0 0 1 1-1c5.523 0 10 4.477 10 10a1 1 0 1 1-2 0 8 8 0 0 0-8-8 1 1 0 0 1-1-1zm0 4a1 1 0 0 1 1-1 6 6 0 0 1 6 6 1 1 0 1 1-2 0 4 4 0 0 0-4-4 1 1 0 0 1-1-1z"
                      ></path>
                    </svg>
                  {{/if}}
                  {{this.text}}
                </a>

              </li>
            {{/each}}
          </ul>
        {{/each}}
      </div>
    </div>
  </div>
  <div class="container">
    <div class="footer__copyright">
      Â© CHAVES BILBAO, S. L.
    </div>
  </div>
</footer>